{
  "name": "lena-student-web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx @react-router/dev build",
    "dev": "npx @react-router/dev dev",
    "start": "npx @react-router/serve ./build/server/index.js",
    "typecheck": "npx @react-router/dev typegen && tsc",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "deploy": "npm run build && wrangler deploy",
    "deploy:dev": "npm run build && wrangler deploy --env dev",
    "workers:dev": "npm run build && wrangler dev",
    "pages:deploy": "npm run build && wrangler pages deploy build/client",
    "pages:dev": "npm run build && wrangler pages dev build/client",
    "pages:secret": "wrangler pages secret put"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-router/cloudflare": "^7.8.2",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "isbot": "^5.1.27",
    "lucide-react": "^0.541.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.7",
    "@cloudflare/workers-types": "^4.20250823.0",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/cookie": "^0.6.0",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.32.0"
  }
}
